    "# Размеры когорт\n",
    "cohort_size = df_result.groupby('cohort', as_index=False).agg({id_type: 'max', 'paid_users': 'max'})\n",
    "df_result = df_result.merge(cohort_size, on='cohort'). \\\n",
    "    rename(\n",
    "    columns={f'{id_type}_x': 'num_users', f'{id_type}_y': 'cohort_size',\n",
    "                'paid_users_x': 'paid_users', 'paid_users_y': 'paid_size'})\n",
    "# Так присоединим накопительный доход\n",
    "df_result = df_result.merge(cum_revenue, how='left', left_on=['cohort', 'days_after_install'],\n",
    "                            right_on=['cohort', 'days_after_install'])\n",
    "df_result"
   ]
