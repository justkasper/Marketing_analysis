    "# Imports and Colab tables\n",
    "import pandas as pd\n",
    "import datetime as dt\n",
    "from datetime import timedelta\n",
    "from clickhouse_driver import Client"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 43,
   "metadata": {
    "id": "lnndXNOAtz2k"
   },
   "outputs": [],
   "source": [
    "def data_get(platform: str, start_date: str, finish_date: str, game_type='free'):\n",
    "    \"\"\"\n",
    "    Connect to Clickhouse db and get games data including: game name (app_info__id),\n",
    "    both player id's (idfa and user_pseudo_id), event_name in ['session_start', 'in_app_purchase'],\n",
    "    event_timestamp with date, user_first_touch_timestamp and event_value_in_usd (revenue)\n",
    "    for time period in [start_date, finish_date] range.\n",
    "\n",
    "    :param platform: str, in ['android', 'ios'] - name of the mobile platform\n",
    "    :param start_date: str, like 'yyyy-mm-dd' - starting date of period, this date is included in result\n",
    "    :param finish_date: str, like 'yyyy-mm-dd' - end date of period, this date is included in result\n",
    "    :param game_type: str, in ['free', 'premium', 'all'] - game type\n",
    "    :return: pd.DataFrame object\n",
    "    \"\"\"\n",
    "\n",
    "    client = Client(host='host', port='5342', user='user_read',\n",
    "                    password='password', database='firebase_events')\n",
    "\n",
    "    # Check if dates are strings\n",
    "    start_date = str(start_date)\n",
    "    finish_date = str(finish_date)\n",
    "\n",
    "    if game_type == 'free':\n",
    "        sql_where_app_add = f\"like '%free2%'\"\n",
    "    elif game_type == 'premium':\n",
    "        sql_where_app_add = f\"not like '%free2%'\"\n",
    "    else:\n",
    "        sql_where_app_add = ''\n",
    "\n",
    "    sql_query = f\"\"\"\n",
    "        select  app_info__id, JSONExtractString(device, 'advertising_id') as idfa,\n",
    "                user_pseudo_id,  user_first_touch_timestamp, event_timestamp, event_name, event_value_in_usd\n",
    "        from {platform}1\n",
    "        where event_date between '{start_date}' and '{finish_date}'\n",
    "            and event_name in ('session_start', 'in_app_purchase')\n",
    "            and app_info__id {sql_where_app_add}\n",
    "        union all\n",
    "        select  app_info__id, JSONExtractString(device, 'advertising_id') as idfa,\n",
    "                user_pseudo_id,  user_first_touch_timestamp, event_timestamp, event_name, event_value_in_usd\n",
    "        from {platform}2\n",
    "        where event_date between '{start_date}' and '{finish_date}'\n",
    "            and event_name in ('session_start', 'in_app_purchase')\n",
    "            and app_info__id {sql_where_app_add}\n",
    "        union all\n",
    "        select  app_info__id, JSONExtractString(device, 'advertising_id') as idfa,\n",
    "                user_pseudo_id,  user_first_touch_timestamp, event_timestamp, event_name, event_value_in_usd\n",
    "        from {platform}3\n",
    "        where event_date between '{start_date}' and '{finish_date}'\n",
    "            and event_name in ('session_start', 'in_app_purchase')\n",
    "            and app_info__id {sql_where_app_add}\n",
    "        \"\"\"\n",
    "\n",
    "    result, columns = client.execute(sql_query, with_column_types=True)\n",
    "    data = pd.DataFrame(result, columns=[row[0] for row in columns])\n",
    "\n",
    "    return data"
   ]
  },
