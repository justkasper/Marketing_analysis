   "source": [
    "# Rename merged columns and add LTV metric\n",
    "df_result = df_result.rename(columns={'event_value_in_usd': 'daily_revenue'})\n",
    "df_result['ltv'] = df_result['cumulative_revenue'] / df_result['cohort_size']\n",
    "df_result['ltv_paid'] = df_result['cumulative_revenue'] / df_result['paid_size']\n",
    "df_result['retention'] = df_result['num_users'] / df_result['cohort_size']\n",
    "df_result"
   ]
  },
