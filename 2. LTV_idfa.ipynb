   "source": [
    "def data_prepare(data):\n",
    "    \"\"\"\n",
    "    Convert date string fields to datetime, drop duplicates, add dates and id's.\n",
    "\n",
    "    :param data: pd.DataFrame from function data_get\n",
    "    :param id_type:\n",
    "    :return:    sorted copy and converted of raw data as pd.DataFrame, unified id's\n",
    "                with additional columns ['event_date', 'first_open_date', 'id']\n",
    "    :return: games_list: list of all games for further manual filtering\n",
    "    :return: paid_users: array of users that have bought any IAPs\n",
    "    \"\"\"\n",
    "\n",
    "    df = data.copy(deep=True)\n",
    "    df = df.drop_duplicates()\n",
    "\n",
    "    # Format string columns to datetime format\n",
    "    df['event_timestamp'] = pd.to_datetime(df['event_timestamp'], infer_datetime_format=True)\n",
    "    df['user_first_touch_timestamp'] = pd.to_datetime(df['user_first_touch_timestamp'], infer_datetime_format=True)\n",
    "\n",
    "    # Create date columns with 3 hours shift (server time offset)\n",
    "    df['event_date'] = pd.to_datetime((df['event_timestamp'] + timedelta(hours=3)).dt.date, format=\"%Y/%m/%d\")\n",
    "    df['first_open_date'] = pd.to_datetime((df['user_first_touch_timestamp'] + timedelta(hours=3)).dt.date,\n",
    "                                           format=\"%Y/%m/%d\")\n",
    "    # Convert revenue values to float (decimal to float)\n",
    "    df['event_value_in_usd'] = df['event_value_in_usd'].astype('float')\n",
    "\n",
    "    return df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 45,
