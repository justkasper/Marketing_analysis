    "id": "Nwwzju-IOIJ-"
   },
   "outputs": [],
   "source": [
    "def add_cohorts(df):\n",
    "    \"\"\"\n",
    "    Create cohorts based of first_open_date and event_date. First filter only users started playing games after\n",
    "    min event dates. Then make cohorts based on user's earliest event -\n",
    "    that's needed because some users have events registered before first_open_date\n",
    "\n",
    "    :param df: pd.DataFrame with columns ['app_info__id', 'id', 'event_date', 'first_open_date']\n",
    "            Result of get and prepare functions for DG ClickHouse database\n",
    "    :param id_type:\n",
    "    :return: df with added column ['cohorts']\n",
    "    \"\"\"\n",
    "    df_temp = df.copy()\n",
    "    df_temp = df_temp[df_temp['first_open_date'] >= df_temp['event_date'].min()]\n",
    "\n",
    "    # Fix first_open_date assigning session start date where there's difference between first touch and actual use\n",
    "    df_temp = df_temp[df_temp['event_name'] == 'session_start'].groupby(['app_info__id', 'user_pseudo_id'],\n",
    "                              as_index=False)['event_date'].min().rename(columns={'event_date': 'cohort'})\n",
    "    df = df.merge(df_temp, left_on=['app_info__id', 'user_pseudo_id'], \n",
    "                  right_on=['app_info__id', 'user_pseudo_id'], how='left')\n",
    "\n",
    "    return df"
   ]
  },
